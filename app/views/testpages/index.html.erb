<p id="notice"><%= notice %></p>

<h1>Testpages</h1>

<div class="row" style="padding-top:20px;">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        列表 <%= link_to '新建', new_testpage_path %>
      </div>
      <!-- /.panel-heading -->
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-hover">
            <thead>
              <tr>
                                  <th>name</th>
                                  <th>count</th>
                                  <th>miaoshu</th>
                                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @testpages.each do |testpage| %>
                <tr>
                    <td><%=  testpage.name %></td>
                    <td><%= testpage.count %></td>
                    <td><%= testpage.description %></td>
                    <td><%= link_to '查看', testpage_path(testpage) %></td>
                  <td><%= link_to '编辑', edit_testpage_path(testpage) %></td>
                  <td><%= link_to '删除', testpage, method: :delete, data: { confirm: '确定么?' } %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

<%=will_paginate @testpages,renderer: BootstrapPagination::Rails%>